{
  "name": "teloi-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "zed_diff": "git diff --staged --full-index > commit.txt | zed commit.txt",
    "prepare": "husky",
    "dev:web": "pnpm -F @teloi/web dev"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  },
  "devDependencies": {
    "@teloi/typescript-config": "workspace:*",
    "@types/node": "^24.3.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.5.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.30.1"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
